    "require-dev": {
        "elodgy/coding-standard": "^1.0",
        "phpstan/phpstan": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "symfony/var-dumper": "^5.3",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan-phpunit": "^1.0",
        "php-parallel-lint/php-var-dump-check": "^0.5.0"
