{
    "name": "elodgy/library-template",
    "description": "E-Lodgy library template",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "benjaminmal",
            "email": "bbenben778@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "elodgy/coding-standard": "^1.0",
        "phpstan/phpstan": "^1.2",
        "phpstan/extension-installer": "^1.1",
        "symfony/var-dumper": "^5.3 || ^6.0",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan-phpunit": "^1.0",
        "php-parallel-lint/php-var-dump-check": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "Elodgy\\Library\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elodgy\\Library\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "mv .gitattributes.disable .gitattributes"
        ]
    }
}
